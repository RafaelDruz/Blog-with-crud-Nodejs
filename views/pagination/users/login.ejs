
<%- include ('../../partials/header.ejs') %>

<section class="LoginForm" id="contact">
    <a href="/" class="logoout"><h2>Retornar</h2></a>
    <form action="/authenticate" method="POST" class="loginform">
      <h1 class="heading"> Login </h1>
      <label for="username">Email:</label>
      <input type="email" id="username" placeholder="email" class="box"required name="email">
      <label for="password">Password:</label>
      <input type="password" id="password" name="password" placeholder="*******" required>
      <input type="submit" id="my-form-button" value="Login">
      <div id="my-form-status" class="inf_status"></div>
   </form>
   <h3>Se não tiver cadastros, então crie um <a href="/users/create"><span>aqui</span></a></h3>
</section>


 